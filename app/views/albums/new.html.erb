<div class= "text-center">
  <div class="container">
    <div class="row">
      <div class="col-xs-4 col-xs-offset-4">
        <%= simple_form_for [@rider, @album] do |f| %>
        <%= f.input :title %>
        <%= f.input :photographer %>
        <%= f.input :location %>
        <%= f.input :photos, as: :attachinary %>
        <%= f.submit class: "btn btn-bro" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
